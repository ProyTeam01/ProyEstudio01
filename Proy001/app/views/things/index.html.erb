<p id="notice"><%= notice %></p>

<h1>Listing Things</h1>

<table>
  <thead>
    <tr>
      <th>Ippadre</th>
      <th>Nivel</th>
      <th>Titulo</th>
      <th>Content</th>
      <th>Createdby</th>
      <th>Upvote</th>
      <th>Downvote</th>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @things.each do |thing| %>
      <tr>
        <td><%= thing.ippadre %></td>
        <td><%= thing.nivel %></td>
        <td><%= thing.titulo %></td>
        <td><%= thing.content %></td>
        <td><%= thing.createdby %></td>
        <td><%= thing.upvote %></td>
        <td><%= thing.downvote %></td>
        <td><%= thing.type %></td>
        <td><%= link_to 'Show', thing %></td>
        <td><%= link_to 'Edit', edit_thing_path(thing) %></td>
        <td><%= link_to 'Destroy', thing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Thing', new_thing_path %>
